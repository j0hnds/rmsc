<%= form_for @coordinator do |f| %>
  <%= field_set_tag do %>
  <ul>
    <li><%= form_label_field f, :first_name %></li>
    <li><%= form_label_field f, :last_name %></li>
    <li><%= form_label_field f, :phone %></li>
    <li><%= form_label_field f, :email %></li>
  </ul>
  <% end %>
  <%= submit_tag 'Save Coordinator' %>
<% end %>
